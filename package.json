{
  "name": "github-emoji-picker",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "all": "npm run build && npm run format && npm run lint && npm run test:ci",
    "build": "react-scripts build",
    "eject": "react-scripts eject",
    "format": "prettier --write \"**/*.ts\"",
    "format:check": "prettier --check \"**/*.ts\"",
    "lint": "eslint \"**/*.ts\"",
    "start": "react-scripts start",
    "test": "react-scripts test --transformIgnorePatterns \"/node_modules/(?!react-github-btn/)\"",
    "test:ci": "react-scripts test --watchAll=false --transformIgnorePatterns \"/node_modules/(?!react-github-btn/)\"",
    "update-emojis": "node scripts/create_github_emoji_list.js"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "dependencies": {
    "@emoji-mart/data": "^1.1.0",
    "@emoji-mart/react": "^1.1.0",
    "@emotion/react": "^11.10.0",
    "@emotion/styled": "^11.10.0",
    "@mui/material": "^5.11.0",
    "@octokit/core": "^4.1.0",
    "emoji-datasource": "^14.0.0",
    "emoji-mart": "^5.5.1",
    "emojilib": "^3.0.7",
    "inflection": "^2.0.0",
    "octokit": "^2.0.7",
    "react-dom": "^18.2.0",
    "react-github-btn": "^1.3.0",
    "unicode-emoji-json": "^0.4.0",
    "web-vitals": "^3.1.0"
  },
  "devDependencies": {
    "@babel/runtime": "^7.20.1",
    "@testing-library/dom": "8.20.0",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^13.4.0",
    "@testing-library/user-event": "^14.4.3",
    "@types/emoji-mart": "^3.0.9",
    "@types/jest": "^29.2.0",
    "@types/node": "^18.11.2",
    "@types/react": "^18.0.20",
    "@types/react-dom": "^18.0.6",
    "prettier": "^2.8.0",
    "react-scripts": "5.0.1",
    "sort-package-json": "^2.2.0",
    "typescript": "^4.9.3"
  }
}
